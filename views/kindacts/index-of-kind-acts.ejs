<!-- INDEX PAGE TEMPLATE / DISPLAY ALL KIND ACTS -->
 <!-- index-of-kind-acts -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Index of Kind Acts</title>
</head>
<body>
    <h1>All Kind Acts</h1>
    <ul>
        <% kindacts.forEach(kindact => { %>
            <li><%= kindact.title %></li>
        <% }); %>
    </ul>   

    <!-- ! Need to add a href links at the bottom of the webpage to interconnect pages -->
    <a href="/kindacts">Browse Kind Acts</a> <br>
    <a href="/kindacts/new">Add New Kind Act</a> <br>
    <a href="/kindacts/edit">Update A Kind Act</a> <br>
    <a href="/kindacts/show">Remove A Kind Act</a> <br>


</body>
</html>